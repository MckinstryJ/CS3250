	private String professorTable = "<table class = \"table table-responsive bg-primary\" style=\"width: 80%; align: center\">"
			+ "					<tr>"
			+ "						<th>Student ID #</th>"	
			+ "						<th>First Name</th>"
			+ "						<th>Last Name</th>"
			+ "						<th>Options</th>"
			+ "					</tr>";
	
	@RequestMapping("/Professors")
	public String home() {
